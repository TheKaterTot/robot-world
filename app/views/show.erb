<h3><a href="/robots">Robot Directory</a></h3>
<h3><a href="/robots/<%= @robot.id %>/edit">Edit Robot</a></h3>
<form action="/robots/<%= @robot.id %>" method="POST">
    <input type="hidden" name="_method" value="DELETE">
    <h1><%= @robot.name %></h1>
    <p><%= @robot.city %></p>
    <p><%= @robot.state %></p>
    <p><%= @robot.department %></p>
    <input class="btn btn-primary" type="submit" value="delete"/>
  </form>
